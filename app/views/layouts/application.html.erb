<!DOCTYPE html>
<html>
<head>
  <%= favicon_link_tag 'favicon.ico' %>
  <title>Le RÃ©seau - Philanthropos</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
  <script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>
  <%= csrf_meta_tags %>
  <% if content_for?(:head) %>
    <%= yield(:head) %>
  <% end %>
</head>
<body>
  <!-- <div class="container"> -->
    <% if current_user.present? %>
      <div id="header">
        <div class="container">
          <%= render 'layouts/partial/head_content'%>
        </div>  
      </div>
      <div id="main">
        <div class="container">
          <div class="row">
            <div class="col-md-9 ">
              <div class="pre_main_content">
                <%= render 'layouts/partial/app_slider' %>
              </div>
              <div class="main_content">
                <%= render 'layouts/partial/flash' %>
                <%= yield %>
              </div>
          </div>
          <div class="col-md-3 right_nav_section pull-right">
            <%= render 'layouts/partial/right_nav_section'%>
          </div>
        </div>
      </div>
    </div>
    <% else %>
      <div id="#main">
        <div class="container">
          <%= render 'layouts/partial/flash' %>
          <%= yield %>
        </div>
      </div>
    <% end %>
    <div id="footer">
      <div class="container">
        
      </div>
    </div>
  <!-- </div> -->
</body>
</html>
<!-- google analytics -->
<%= render 'layouts/partial/google_analytics' %>
